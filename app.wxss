@import "styles/iconfont.wxss";
page, view, text, swiper, swiper-item, image, navigator{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
  }
  page {
    /* 主题颜色 */
    --themeColor:#F79F1F;
    /* 定义统一的字体大小 假如设计稿是375px 
      1px = 2rpx
      14px = 28rpx
    */
    font-size: 28rpx;
  }